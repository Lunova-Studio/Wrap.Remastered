{
  "Wrap": {
    "Console": {
      "IsEnableCommand": true,
      "OutputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss} {Level:u3}] ({SourceContext}): {Message}{NewLine}{Exception}",
      "MinimumLevel": {
        "Default": "Information"
      }
    }
  },

  "HostOptions": {
    "ShutdownTimeout": "00:00:30"
  }
}